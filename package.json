{
  "name": "prompting",
  "private": true,
  "version": "v.3.2",
  "type": "module",
  "packageManager": "bun@1.3.4",
  "homepage": "https://corbaz.github.io/web-chat",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corbaz/web-chat.git"
  },
  "author": "Julio Corbaz <julio.corbaz@gmail.com>",
  "bugs": {
    "url": "https://github.com/corbaz/web-chat/issues"
  },
  "license": "MIT",
  "keywords": [
    "web chat",
    "AI",
    "Groq"
  ],
  "scripts": {
    "bun-install": "bun install",
    "bun-upgrade": "bun upgrade && bunx -y npm-check-updates",
    "ncu": "bunx -y npm-check-updates -u && bun update",
    "dev": "vite --host",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "build": "tsc -b && vite build --outDir ./docs",
    "preview": "vite preview --host",
    "deploy": "surge docs --domain deepchat.surge.sh"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-select": "^5.10.2",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "sweetalert2": "^11.26.10",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.25",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.3.0"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "esbuild"
  ]
}
