        "react-select": "^5.10.1",
        "sweetalert2": "^11.21.0",
        "tailwindcss": "^4.1.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.26.0",
        "@types/react": "^19.1.4",
        "@types/react-dom": "^19.1.5",
        "@vitejs/plugin-react": "^4.4.1",
        "eslint": "^9.26.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.1.0",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.32.1",
        "vite": "^6.3.5"
