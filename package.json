        "preview": "vite preview --host",
        "deploy": "surge docs --domain deepchat.surge.sh"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.6",
        "axios": "^1.9.0",
        "react": "^19.1.0",
