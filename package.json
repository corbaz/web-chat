{
    "name": "web-chat",
    "private": true,
    "version": "v.2.7",
    "type": "module",
    "scripts": {
        "bun-install": "bun install",
        "bun-upgrade": "bun upgrade && bunx -y npm-check-updates",
        "ncu": "npx -y npm-check-updates -u && bun update",
        "dev": "vite --host",
        "build": "tsc -b && vite build --outDir ./docs",
